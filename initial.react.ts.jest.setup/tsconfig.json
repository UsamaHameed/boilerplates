{
  "compilerOptions": {
    "target": "esnext",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "src",
    "baseUrl": "src"
  },
  "include": ["src"]
}
